    "dotenv": "^8.0.0",
    "event-stream": "^4.0.1",
    "json2csv": "^4.5.1",
    "moleculer": "^0.13.0",
    "moleculer-web": "^0.8.0",
    "moment": "^2.24.0",
    "mysql": "^2.17.1",
    "nats": "^1.3.0",
    "rets-client": "^5.2.3",
    "superagent": "^5.1.0",
    "through2": "^3.0.1",
    "wpapi": "^1.2.1"
  },
  "engines": {
